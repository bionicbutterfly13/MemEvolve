# === Core Configuration (Required) ===
DEFAULT_MODEL="gpt-5-mini"                       # Default model for all operations
OPENAI_API_KEY="your_openai_api_key_here"       # OpenAI API key

# === API Base URLs ===
OPENAI_BASE_URL="https://api.openai.com/v1" 

# === Search & Crawl Tools (Required only for running Flash-Searcher agents) ===
SERPER_API_KEY="your_serper_api_key_here"       # SerpApi for web search (https://serper.dev/)

# Web crawling - Choose ONE of the following:
# Option 1: JinaAI (requires API key)
JINA_API_KEY="your_jina_api_key_here"           # JinaAI for web crawling (https://jina.ai/)
WEB_ACCESS_PROVIDER="jina"                      # Default provider

# Option 2: crawl4ai (free, open-source, no API key needed)
# WEB_ACCESS_PROVIDER="crawl4ai"                # Use crawl4ai instead of Jina

# === Optional: Override specific models ===
 ANALYSIS_MODEL="gpt-5"                         # Model for trajectory analysis (default: DEFAULT_MODEL)
 GENERATION_MODEL="gpt-5"                       # Model for system generation (default: DEFAULT_MODEL)
 MSWEA_MODEL_NAME="gpt-5-mini"                  # Model for auto-fixing (default: DEFAULT_MODEL)

# === Dionysus3 Integration (Optional - for persistent graph memory) ===
# Enable integration with Dionysus3-core for Neo4j/Graphiti memory storage
# DIONYSUS_WEBHOOK_BASE_URL="https://your-vps.com/webhook/memevolve/v1"
# DIONYSUS_HMAC_SECRET="your-shared-secret-key-min-32-chars"
# DIONYSUS_PROJECT_ID="memevolve-production"
# DIONYSUS_ENVIRONMENT="development"            # development/staging/production

# Feature flags (all default to true)
# DIONYSUS_ENABLED="true"                       # Master switch
# DIONYSUS_GRAPH_WRITES_ENABLED="true"          # Enable Neo4j/Graphiti writes
# DIONYSUS_VECTOR_WRITES_ENABLED="true"         # Enable pgvector writes
# DIONYSUS_ENTITY_EXTRACTION="true"             # Extract entities from trajectories
# DIONYSUS_SESSION_TRACKING="true"              # Track session for continuity

# Timeouts (in seconds)
# DIONYSUS_CONNECT_TIMEOUT="2.0"
# DIONYSUS_RECALL_TIMEOUT="10.0"
# DIONYSUS_INGEST_TIMEOUT="20.0"

# Canary deployment (limit to specific project for testing)
# DIONYSUS_CANARY_PROJECT="test-project-id"